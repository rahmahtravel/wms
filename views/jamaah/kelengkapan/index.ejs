  <script>
    // Statistics Toggle Functionality
    let isStatsVisible = true;

    function toggleStats() {
      const statsCards = document.getElementById('statsCards');
      const statsToggle = document.getElementById('statsToggle');
      const statsChevron = document.getElementById('statsChevron');
      const toggleText = statsToggle.querySelector('span');
      
      if (isStatsVisible) {
        // Hide stats
        statsCards.classList.add('stats-collapsed');
        statsCards.classList.remove('stats-expanded');
        statsChevron.classList.remove('fa-chevron-up');
        statsChevron.classList.add('fa-chevron-down');
        toggleText.textContent = 'Tampilkan';
        
        // Store preference
        localStorage.setItem('jamaah-stats-visible', 'false');
      } else {
        // Show stats
        statsCards.classList.remove('stats-collapsed');
        statsCards.classList.add('stats-expanded');
        statsChevron.classList.remove('fa-chevron-down');
        statsChevron.classList.add('fa-chevron-up');
        toggleText.textContent = 'Sembunyikan';
        
        // Store preference
        localStorage.setItem('jamaah-stats-visible', 'true');
      }
      
      isStatsVisible = !isStatsVisible;
    }

    // Legend Toggle Functionality
    let isLegendVisible = true;

    function toggleLegend() {
      const legendContent = document.getElementById('legendContent');
      const legendChevron = document.getElementById('legendChevron');
      const legendToggleText = document.getElementById('legendToggleText');
      
      if (isLegendVisible) {
        // Hide legend
        legendContent.classList.add('legend-content-collapsed');
        legendContent.classList.remove('legend-content-expanded');
        legendChevron.classList.remove('fa-chevron-up');
        legendChevron.classList.add('fa-chevron-down');
        legendToggleText.textContent = 'Tampilkan';
        
        // Store preference
        localStorage.setItem('jamaah-legend-visible', 'false');
      } else {
        // Show legend
        legendContent.classList.remove('legend-content-collapsed');
        legendContent.classList.add('legend-content-expanded');
        legendChevron.classList.remove('fa-chevron-down');
        legendChevron.classList.add('fa-chevron-up');
        legendToggleText.textContent = 'Sembunyikan';
        
        // Store preference
        localStorage.setItem('jamaah-legend-visible', 'true');
      }
      
      isLegendVisible = !isLegendVisible;
    }

    // Enhanced Number Animation with Easing
    function animateNumberEnhanced(elementId, targetNumber, duration = 1500) {
      const element = document.getElementById(elementId);
      if (!element) return;
      
      const start = 0;
      const end = parseInt(targetNumber) || 0;
      const startTime = performance.now();
      
      // Add loading class for pulse effect
      element.closest('.group')?.classList.add('card-loading');
      
      function easeOutQuart(t) {
        return 1 - Math.pow(1 - t, 4);
      }
      
      function update(currentTime) {
        const elapsed = currentTime - startTime;
        const progress = Math.min(elapsed / duration, 1);
        const easedProgress = easeOutQuart(progress);
        
        const current = Math.floor(start + (end - start) * easedProgress);
        element.textContent = current.toLocaleString('id-ID');
        
        // Add scale effect during animation
        const scale = 1 + (Math.sin(progress * Math.PI) * 0.05);
        element.style.transform = `scale(${scale})`;
        
        if (progress < 1) {
          requestAnimationFrame(update);
        } else {
          element.style.transform = 'scale(1)';
          // Remove loading class
          element.closest('.group')?.classList.remove('card-loading');
        }
      }
      
      requestAnimationFrame(update);
    }

    // Load paket umroh data and statistics on page load
    document.addEventListener('DOMContentLoaded', function() {
      // Restore stats visibility preference
      const savedVisibility = localStorage.getItem('jamaah-stats-visible');
      if (savedVisibility === 'false') {
        // Delay to ensure DOM is ready
        setTimeout(() => {
          toggleStats();
        }, 100);
      }
      
      // Restore legend visibility preference
      const savedLegendVisibility = localStorage.getItem('jamaah-legend-visible');
      if (savedLegendVisibility === 'false') {
        setTimeout(() => {
          toggleLegend();
        }, 100);
      }
      
      loadPaketUmrohFilter();
      loadStatistics();
      loadAllChecklistItems();
      
      // Apply auto-hide for 'Sudah Diambil' rows if no filters are active
      applyAutoHideSudahDiambil();
      
      // Add smooth scroll behavior for stats cards
      const observer = new IntersectionObserver((entries) => {
        entries.forEach((entry) => {
          if (entry.isIntersecting) {
            entry.target.classList.add('animate-fadeInUp');
          }
        });
      }, { threshold: 0.1 });
      
      // Observe stats cards for animation
      document.querySelectorAll('.group').forEach((card) => {
        observer.observe(card);
      });
    });

    async function loadPaketUmrohFilter() {
      try {
        const response = await fetch('/jamaah/paket-umroh-list');
        const paketData = await response.json();
        
        const filterPaket = document.getElementById('filterPaket');
        
        // Clear existing options (keep "Semua Paket")
        filterPaket.innerHTML = '<option value="">Semua Paket</option>';
        
        // Group paket by month and populate options
        Object.keys(paketData).forEach(monthYear => {
          const optgroup = document.createElement('optgroup');
          optgroup.label = monthYear;
          
          paketData[monthYear].forEach(paket => {
            const option = document.createElement('option');
            option.value = paket.id;
            option.textContent = `${paket.nama_paket} - Batch ${paket.batch} (${new Date(paket.tanggal_keberangkatan).toLocaleDateString('id-ID', { day: '2-digit', month: 'long', year: 'numeric' })})`;
            optgroup.appendChild(option);
          });
          
          filterPaket.appendChild(optgroup);
        });
        
        // Set selected value from server-side filter
        const selectedPaket = '<%= filters.paket %>';
        if (selectedPaket) {
          filterPaket.value = selectedPaket;
        }
        
      } catch (error) {
        console.error('Error loading paket umroh data:', error);
      }
    }

    async function loadStatistics() {
      try {
        // Get current filter parameters
        const params = new URLSearchParams(window.location.search);
        const response = await fetch(`/jamaah/kelengkapan-stats?${params.toString()}`);
        const stats = await response.json();
        
        // Update statistics with enhanced animation
        animateNumberEnhanced('totalJamaah', stats.totalJamaah, 1200);
        setTimeout(() => animateNumberEnhanced('belumDiambil', stats.belumDiambil, 1000), 200);
        setTimeout(() => animateNumberEnhanced('sudahDiambil', stats.sudahDiambil, 1000), 400);
        setTimeout(() => animateNumberEnhanced('dalamPengiriman', stats.dalamPengiriman || 0, 1000), 600);
        setTimeout(() => animateNumberEnhanced('infantJamaah', stats.infantJamaah || 0, 1000), 800);
        setTimeout(() => animateNumberEnhanced('totalInfantCount', stats.infantJamaah || 0, 800), 100);
        
      } catch (error) {
        console.error('Error loading statistics:', error);
        // Set fallback values with animation
        animateNumberEnhanced('totalJamaah', 0);
        animateNumberEnhanced('belumDiambil', 0);
        animateNumberEnhanced('sudahDiambil', 0);
        animateNumberEnhanced('dalamPengiriman', 0);
        animateNumberEnhanced('infantJamaah', 0);
        animateNumberEnhanced('totalInfantCount', 0);
      }
    }



    function openModal(modalId) {
      document.getElementById(modalId).classList.remove('hidden');
      document.getElementById('kelengkapanForm').reset();
      document.getElementById('itemId').value = '';
      document.getElementById('modalTitle').textContent = 'Tambah Kelengkapan Jamaah';
    }

    function closeModal(modalId) {
      document.getElementById(modalId).classList.add('hidden');
    }

    function viewDetail(id) {
      window.location.href = `/jamaah/kelengkapan/${id}`;
    }

    function editItem(id) {
      fetch(`/jamaah/kelengkapan/${id}`)
        .then(res => res.json())
        .then(data => {
          document.getElementById('itemId').value = data.id;
          document.getElementById('nama_jamaah').value = data.nama_jamaah;
          document.getElementById('nomor_telp').value = data.nomor_telp;
          document.getElementById('status').value = data.status;
          document.getElementById('keterangan').value = data.keterangan || '';
          document.getElementById('modalTitle').textContent = 'Edit Kelengkapan Jamaah';
          openModal('createModal');
        });
    }

    function deleteItem(id) {
      swalConfirmFetch(`/jamaah/kelengkapan/${id}`, { method: 'DELETE' }, 'Data berhasil dihapus', 'Gagal menghapus data');
    }

    document.getElementById('kelengkapanForm').addEventListener('submit', function(e) {
      e.preventDefault();
      const formData = new FormData(this);
      const id = document.getElementById('itemId').value;
      const url = id ? `/jamaah/kelengkapan/${id}` : '/jamaah/kelengkapan';
      const method = id ? 'PUT' : 'POST';
      // Ensure name is passed into kepala_keluarga_id fallback field for DB
      if (formData.get('nama_jamaah') && !formData.get('kepala_keluarga_id')) {
        formData.append('kepala_keluarga_id', formData.get('nama_jamaah'));
      }

      fetch(url, {
        method: method,
        headers: {
          'Content-Type': 'application/json',
        },
        body: JSON.stringify(Object.fromEntries(formData))
      })
      .then(res => res.json())
      .then(data => {
        if (data && data.success) {
          swalSuccess('Data berhasil disimpan', true);
        } else {
