    // Filter functionality - now uses server-side filtering
    let filterTimeout;
    document.getElementById('searchInput').addEventListener('input', debounceFilter);
    document.getElementById('filterStatus').addEventListener('change', applyFilter);
    document.getElementById('filterPaket').addEventListener('change', applyFilter);

    function debounceFilter() {
      clearTimeout(filterTimeout);
      filterTimeout = setTimeout(applyFilter, 500); // Wait 500ms after user stops typing
    }

    function applyFilter() {
      const searchTerm = document.getElementById('searchInput').value;
      const statusFilter = document.getElementById('filterStatus').value;
      const paketFilter = document.getElementById('filterPaket').value;
      
      // Build URL with filter parameters
      const params = new URLSearchParams();
      if (searchTerm) params.set('search', searchTerm);
      if (statusFilter) params.set('status', statusFilter);
      if (paketFilter) params.set('paket', paketFilter);
      params.set('page', '1'); // Reset to page 1 when filtering
      
      // Redirect with filters
      window.location.href = `${window.location.pathname}?${params.toString()}`;
      
      // Also reload statistics with new filters
      loadStatistics();
    }

    function getFilterUrl(page) {
      const params = new URLSearchParams(window.location.search);
      params.set('page', page);
      return `${window.location.pathname}?${params.toString()}`;
    }

    function navigateToPage(page) {
      const params = new URLSearchParams(window.location.search);
      params.set('page', page);
      window.location.href = `${window.location.pathname}?${params.toString()}`;
      return false; // Prevent default link behavior
    }

    function resetFilters() {
      // Redirect to page without any filter parameters
      window.location.href = window.location.pathname;
    }
    
    // Function to hide 'Sudah Diambil' rows when no filters are active
    function applyAutoHideSudahDiambil() {
      // Check if any filter is active
      const searchTerm = document.getElementById('searchInput').value.trim();
      const statusFilter = document.getElementById('filterStatus').value;
      const paketFilter = document.getElementById('filterPaket').value;
      
      const hasActiveFilter = searchTerm !== '' || statusFilter !== '' || paketFilter !== '';
      
      // Get all table rows
      const tableRows = document.querySelectorAll('#tableBody tr');
      
      tableRows.forEach(row => {
        const statusCell = row.querySelector('[data-status-pengambilan]');
        if (statusCell) {
          const statusValue = statusCell.getAttribute('data-status-pengambilan');
          
          // Hide row if status is 'Sudah Diambil' and no filters are active
          if (statusValue === 'Sudah Diambil' && !hasActiveFilter) {
            row.style.display = 'none';
          } else {
            row.style.display = '';
          }
        }
      });
    }
    
    // Function to load checklist items for all jamaah
    async function loadAllChecklistItems() {
      const checklistContainers = document.querySelectorAll('.kelengkapan-items-checklist');
      
      for (const [index, container] of checklistContainers.entries()) {
        const kelengkapanId = container.dataset.kelengkapanId;
        
        // Add small delay between requests to prevent server overload
        if (index > 0) {
          await new Promise(resolve => setTimeout(resolve, 100));
        }
        
        await loadChecklistItems(kelengkapanId);
      }
    }

    // Function to load checklist items for specific jamaah (different from modal items)
    async function loadChecklistItems(kelengkapanId) {
      try {
        const response = await fetch(`/jamaah/kelengkapan/${kelengkapanId}/checklist-items`);
        const data = await response.json();
        
        const container = document.querySelector(`[data-kelengkapan-id="${kelengkapanId}"]`);
        if (!container) return;
        
        // Define the items to check
        const itemsToCheck = [
          'bantal', 'buku', 'congratulation', 'jaket', 'jilbab', 
          'kain', 'koper', 'mukena', 'syal', 'tas paspor', 'tas serut'
        ];
        
        let checkedCount = 0;
        let totalCount = itemsToCheck.length;
        
        // Reset all items first
        itemsToCheck.forEach(itemName => {
          const item = container.querySelector(`[data-item="${itemName}"]`);
          if (item) {
            const checkIcon = item.querySelector('.fa-check');
            const xIcon = item.querySelector('.fa-times');
            const box = item.querySelector('span');
            
            checkIcon.classList.add('hidden');
            xIcon.classList.add('hidden');
            box.classList.remove('bg-green-100', 'border-green-500', 'bg-red-100', 'border-red-500');
            box.classList.add('bg-gray-50', 'border-gray-300');
          }
        });
        
        // Update based on data from server
        if (data.success && data.items) {
          data.items.forEach(item => {
            const itemNameLower = item.nama_barang ? item.nama_barang.toLowerCase().trim() : '';
            
            // Enhanced matching logic for better accuracy
            const matchedItem = itemsToCheck.find(checkItem => {
              const checkItemLower = checkItem.toLowerCase();
              
              // Direct match
              if (itemNameLower === checkItemLower) return true;
              
              // Partial matches with common variations
              const patterns = [
                checkItemLower,
                checkItemLower.replace(' ', ''),
                checkItemLower.replace(' ', '_'),
                checkItemLower.replace(' ', '-')
              ];
              
              return patterns.some(pattern => {
                return itemNameLower.includes(pattern) || 
                       pattern.includes(itemNameLower) ||
                       itemNameLower.replace(/[^a-z]/g, '').includes(pattern.replace(/[^a-z]/g, '')) ||
                       pattern.replace(/[^a-z]/g, '').includes(itemNameLower.replace(/[^a-z]/g, ''));
              });
            });
            
            if (matchedItem) {
              const itemElement = container.querySelector(`[data-item="${matchedItem}"]`);
              if (itemElement && item.is_selected) {
                checkedCount++;
                const checkIcon = itemElement.querySelector('.fa-check');
                const xIcon = itemElement.querySelector('.fa-times');
                const box = itemElement.querySelector('span');
                
                checkIcon.classList.remove('hidden');
                xIcon.classList.add('hidden');
                box.classList.remove('bg-gray-50', 'border-gray-300', 'bg-red-100', 'border-red-500');
                box.classList.add('bg-green-100', 'border-green-500');
              }
            }
          });
          
          // Mark unselected items
          itemsToCheck.forEach(itemName => {
            const itemElement = container.querySelector(`[data-item="${itemName}"]`);
            if (itemElement) {
              const checkIcon = itemElement.querySelector('.fa-check');
              if (checkIcon.classList.contains('hidden')) {
                const xIcon = itemElement.querySelector('.fa-times');
                const box = itemElement.querySelector('span');
                
                xIcon.classList.remove('hidden');
                box.classList.remove('bg-gray-50', 'border-gray-300', 'bg-green-100', 'border-green-500');
                box.classList.add('bg-red-100', 'border-red-500');
              }
            }
          });
        }
        
        // Update summary
        const summary = container.querySelector('.checklist-summary');
        if (summary) {
          const percentage = Math.round((checkedCount / totalCount) * 100);
          summary.innerHTML = `${checkedCount}/${totalCount} items (${percentage}%)`;
          
          // Remove loading spinner
          summary.querySelector('.fa-spinner')?.remove();
          
          if (percentage === 100) {
            summary.classList.remove('text-gray-500', 'text-yellow-600', 'text-red-600');
            summary.classList.add('text-green-600', 'font-semibold');
            summary.innerHTML = `<i class="fas fa-check-circle mr-1"></i>${checkedCount}/${totalCount} items (${percentage}%)`;
          } else if (percentage >= 50) {
            summary.classList.remove('text-gray-500', 'text-green-600', 'text-red-600');
            summary.classList.add('text-yellow-600', 'font-medium');
            summary.innerHTML = `<i class="fas fa-exclamation-triangle mr-1"></i>${checkedCount}/${totalCount} items (${percentage}%)`;
          } else {
            summary.classList.remove('text-gray-500', 'text-green-600', 'text-yellow-600');
            summary.classList.add('text-red-600', 'font-medium');
            summary.innerHTML = `<i class="fas fa-times-circle mr-1"></i>${checkedCount}/${totalCount} items (${percentage}%)`;
          }
        }
        
      } catch (error) {
        console.error('Error loading checklist items:', error);
        const container = document.querySelector(`[data-kelengkapan-id="${kelengkapanId}"]`);
        const summary = container ? container.querySelector('.checklist-summary') : null;
        if (summary) {
          summary.innerHTML = '<i class="fas fa-exclamation-triangle mr-1"></i>Error loading';
          summary.classList.remove('text-gray-500', 'text-green-600', 'text-yellow-600');
          summary.classList.add('text-red-500', 'font-medium');
        }
      }
    }

